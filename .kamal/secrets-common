# example content for $HOME/.kamal/xhafanblog_secrets.json
# {
#   "KAMAL_REGISTRY_USERNAME": "<docker hub username>",
#   "KAMAL_REGISTRY_PASSWORD": "<docker hub password>",
# }

SECRETS_FILE="$HOME/.kamal/xhafanblog_secrets.json"

KAMAL_REGISTRY_USERNAME=$(jq -r '.KAMAL_REGISTRY_USERNAME' "$SECRETS_FILE")
KAMAL_REGISTRY_PASSWORD=$(jq -r '.KAMAL_REGISTRY_PASSWORD' "$SECRETS_FILE")
